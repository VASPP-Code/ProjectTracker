    },
    "project_teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-team.project-team",
      "mappedBy": "project_information"
    },
    "project_milestones": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-milestone.project-milestone",
      "mappedBy": "project_information"
    },
    "project_tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-task.project-task",
      "mappedBy": "project_information"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "project_informations"
    }
  }
