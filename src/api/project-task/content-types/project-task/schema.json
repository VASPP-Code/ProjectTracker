{
  "kind": "collectionType",
  "collectionName": "project_tasks",
  "info": {
    "singularName": "project-task",
    "pluralName": "project-tasks",
    "displayName": "project_task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "projectTask_name": {
      "type": "string"
    },
    "projectTask_description": {
      "type": "string"
    },
    "projectTask_status": {
      "type": "enumeration",
      "enum": [
        "was successfully completed",
        "in progress"
      ]
    },
    "projectTask_performedBy": {
      "type": "string"
    },
    "projectTask_startDate": {
      "type": "date"
    },
    "projectTask_ID": {
      "type": "uid"
    },
    "projectTask_plannedHours": {
      "type": "time"
    },
    "projectTask_usedHours": {
      "type": "time"
    },
    "projectTask_delayCode": {
      "type": "enumeration",
      "enum": [
        "need more time",
        "budget is too low",
        "other"
      ]
    },
    "project_milestone": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-milestone.project-milestone",
      "inversedBy": "project_tasks"
    },
    "project_information": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-information.project-information",
      "inversedBy": "project_tasks"
    }
  }
}
