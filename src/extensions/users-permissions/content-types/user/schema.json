{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "usr_firstName": {
      "type": "string"
    },
    "usr_lastName": {
      "type": "string"
    },
    "usr_gender": {
      "type": "enumeration",
      "enum": [
        "w",
        "d",
        "m"
      ]
    },
    "usr_phone": {
      "type": "integer"
    },
    "usr_position": {
      "type": "string"
    },
    "usr_modfDate": {
      "type": "date"
    },
    "user_modfBy": {
      "type": "string"
    },
    "User_ID": {
      "type": "uid"
    },
    "usr_timeentries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::usr-timeentry.usr-timeentry",
      "mappedBy": "user"
    },
    "usr_role": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::usr-role.usr-role",
      "inversedBy": "user"
    },
    "usr_location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::usr-location.usr-location",
      "inversedBy": "user"
    },
    "usr_bankdata": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::usr-bankdata.usr-bankdata"
    },
    "prj_team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::prj-team.prj-team",
      "inversedBy": "users_permissions_users"
    },
    "usr_bankdatum": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::usr-bankdata.usr-bankdata",
      "inversedBy": "users_permissions_user"
    },
    "lve_info": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::lve-info.lve-info",
      "mappedBy": "users_permissions_user"
    }
  }
}
