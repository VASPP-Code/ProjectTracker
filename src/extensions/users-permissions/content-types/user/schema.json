{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "M",
        "F",
        "O"
      ],
      "required": true
    },
    "designation": {
      "type": "string",
      "required": true
    },
    "modifiedOn": {
      "type": "date"
    },
    "modifiedBy": {
      "type": "string"
    },
    "usr_bankdata": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::usr-bankdata.usr-bankdata"
    },
    "department": {
      "type": "enumeration",
      "enum": [
        "    Accounting",
        "    Human Resources",
        "    Marketing",
        "    Sales",
        "    Customer Service",
        "    Information Technology (IT)",
        "    Research and Development (R&D)",
        "    Operations",
        "    Legal",
        "    Administration",
        "    Finance",
        "    Production."
      ],
      "required": true
    },
    "emergencyContName": {
      "type": "string"
    },
    "p_balance_leaves": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::p-balance-leave.p-balance-leave",
      "mappedBy": "users_permissions_users"
    },
    "p_leaves": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::p-leave.p-leave",
      "mappedBy": "users_permissions_user"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "emergencyContPhone": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "zipcode": {
      "type": "integer"
    },
    "appPermission": {
      "type": "json",
      "required": true
    },
    "p_project_teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::p-project-team.p-project-team",
      "mappedBy": "users_permissions_users"
    }
  }
}
